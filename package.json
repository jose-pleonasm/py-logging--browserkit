{
  "name": "py-logging-browserkit",
  "version": "1.2.0",
  "description": "Extension for py-logging",
  "main": "lib/browserkit.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:core": "browserify node_modules/py-logging/core/logging.js --s logging > ./tmp.js && uglifyjs tmp.js -c > ./demo/py-logging.min.js && rm tmp.js",
    "build:commonkit": "browserify node_modules/py-logging/commonkit/index.js --s commonkit > ./tmp.js && uglifyjs tmp.js -c > ./demo/pl-commonkit.min.js && rm tmp.js",
    "build": "uglifyjs ./lib/browserkit.js -c > ./build/browserkit.min.js",
    "doc": "jsdoc2md lib/browserkit.js > API.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jose-pleonasm/py-logging-browserkit.git"
  },
  "keywords": [
    "py-logging",
    "logging",
    "browser"
  ],
  "author": "jose-pleonasm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jose-pleonasm/py-logging-browserkit/issues"
  },
  "homepage": "https://github.com/jose-pleonasm/py-logging-browserkit#readme",
  "devDependencies": {
    "browserify": "^16.2.3",
    "jsdoc-to-markdown": "^4.0.1",
    "py-logging": "^2.0.0",
    "uglifyjs": "^2.4.11"
  }
}
